### Script
$lines($searchall(@,/(IVA \d+% su imponibile di \u20ac %N)/)) {
  %_imp=$search(@,/imponibile di \u20ac (%N)/)
  $if(_imp > $coalesce(imponibile,0)) {
    imponibile=_imp
    iva=$search(@,/IVA (\d\d%)/)
  }
}
